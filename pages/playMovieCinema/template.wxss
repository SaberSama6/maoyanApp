.box{
  height: 232rpx;
  padding: 30rpx 0rpx 0 30rpx;
}
.content{
  padding-bottom: 30rpx;
  border-bottom: 1rpx solid #f2f2f2;
  display: flex;
  justify-content: space-between;
  font-size: 26rpx;
}
.left{
  width: 463rpx;
  display: flex;
  flex-direction: column;
}
.h1{
  font-size: 34rpx; 
}
.cinema{
  display: inline-block;
  white-space: nowrap;
  text-overflow: ellipsis;
   width: 349rpx;
   height: 34rpx;
   overflow: hidden;
}
.money{
  font-size: 26rpx;
  color: #f24e42;
}
.address{
  color: #6c6c6c;
   display: inline-block;
    white-space: nowrap;
    text-overflow: ellipsis;
   width: 349rpx;
   height: 34rpx;
   overflow: hidden;
  margin: 30rpx 0 16rpx 0;
}
.tag>text{
  padding: 5rpx;
  border: 1px solid;
  margin-right: 6rpx;
}
.grey{
  color: #3c8698;
  border-color: #3c8698;
}
.blue{
  background: #88c6ed;
  color: #fff;
}
.origin{
  background: #fdb94e;
  color: #fff;
}
.case{
  padding: 5rpx;
  border: 1px solid;
  background: #fe816f;
  color: #fff;
}
.advert{
  margin-top: 25rpx;
}
.advert_content{
  color: #959595;
}
.right{
  padding-right: 33rpx;
  color: #909090;
  font-size: 22rpx;
  display: flex;
  align-items: center;
}